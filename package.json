  "main": "helloworld.js",
  "scripts": {
    "test": "jest --watchAll  --colors --coverage -- ./tests/unit",
    "test:nowatch": "jest --colors --coverage -- ./tests/unit",
    "test:e2e": "jest --colors -- ./tests/e2e",
    "test:integration": "jest --colors -- ./tests/integration",
    "tsc": "tsc"
  },
  "husky": {
    "hooks": {
      "pre-push": "[[ -z $(git status -s) ]] && tsc --noEmit && npm run test:nowatch || { echo 'Check your TS and make sure you push with no changes or untracked files. You can run *git stash --include-untracked*.' && exit 1; }"
    }
  },
  "dependencies": {
